{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "modules/**",
            "use": "@vercel/static"
        },
        {
            "src": "dashboard_standalone.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/modules/(.*)",
            "dest": "/modules/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}